= form_for @check_code do |f|
  - if @check_code.errors.any?
    #error_explanation
      %h2= "#{pluralize(@check_code.errors.count, "error")} prohibited this check_code from being saved:"
      %ul
        - @check_code.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :code
    = f.text_field :code
  .field
    = f.label :mobile
    = f.text_field :mobile
  .actions
    = f.submit 'Save'
