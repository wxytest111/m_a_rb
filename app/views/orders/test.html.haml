              #DataTables_Table_0_length.dataTables_length
                %label
                  %select{"aria-controls" => "DataTables_Table_0", :name => "DataTables_Table_0_length", :size => "1"}
                    %option{:selected => "selected", :value => "10"} 10
                    %option{:value => "25"} 25
                    %option{:value => "50"} 50
                    %option{:value => "100"} 100
                  records per page
            .col-lg-6
              #DataTables_Table_0_filter.dataTables_filter
                %label
                  Search:
                  %input{"aria-controls" => "DataTables_Table_0", :type => "text"}/
