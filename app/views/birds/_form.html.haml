= form_for @bird do |f|
  - if @bird.errors.any?
    #error_explanation
      %h2= "#{pluralize(@bird.errors.count, "error")} prohibited this bird from being saved:"
      %ul
        - @bird.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
